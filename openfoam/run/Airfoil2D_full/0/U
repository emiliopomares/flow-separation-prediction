FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.009612616959383188 0.002756373558169992 0);

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue uniform (0.009612616959383188 0.002756373558169992 0);
    }

    outlet
    {
        type            freestream;
        freestreamValue uniform (0.009612616959383188 0.002756373558169992 0);
    }

    walls
    {
        type            noSlip;
    }

    frontAndBack
    {
        type            empty;
    }
}

